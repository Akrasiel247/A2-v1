<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seneca.btp400.A2.controllers.WelcomeController">
	<children>
		<Text fontSmoothingType="LCD" layoutX="170.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome" textOrigin="CENTER">
			<font>
				<Font name="Cooper Black" size="57.0" />
			</font>
		</Text>
		<Button fx:id="LoginBtn" layoutX="140.0" layoutY="322.0" mnemonicParsing="false" onAction="#LoginAction" text="User Log In">
			<font>
				<Font name="Cooper Black" size="15.0" />
			</font>
		</Button>
		<Button fx:id="AdminBtn" layoutX="368.0" layoutY="322.0" mnemonicParsing="false" onAction="#AdminLoginAction" text="Admin Log In">
			<font>
				<Font name="Cooper Black" size="15.0" />
			</font>
		</Button>
      <ImageView fx:id="senecaLogo" fitHeight="150.0" fitWidth="200.0" layoutX="175.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
			<image>
			<Image url="https://www.mycism.com/wp-content/uploads/2019/01/37349414_10160459957755391_7708233760965132288_n.png" />
			</image>
		</ImageView>
	</children>
