<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<!--
This scene confirms that the user is who they say they are and lets them know about the rules that they are agreeing to
-->


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="seneca.btp400.A2.controllers.WelcomeUserController">
   <AnchorPane/>
   <Label fx:id="rulesLbl" layoutX="64.0" layoutY="98.0" prefHeight="153.0" prefWidth="470.0" wrapText="true"/>
   <Label fx:id="welcomeMessageLbl" layoutX="62.0" layoutY="34.0" prefHeight="46.0" prefWidth="476.0" wrapText="true">
      <font>
         <Font name="Cooper Black" size="40.0"/>
      </font>
   </Label>
   <CheckBox fx:id="agreeChckbx" layoutX="62.0" layoutY="278.0" mnemonicParsing="false"/>
   <Button fx:id="continueBtn" layoutX="474.0" layoutY="338.0" mnemonicParsing="false" onAction="#changetoVotingScreen"
           text="Continue"/>
   <Label fx:id="alertLbl" layoutX="199.0" layoutY="298.0" prefHeight="20.0" prefWidth="203.0"/>
   <Button fx:id="cancelBtn" layoutX="56.0" layoutY="338.0" mnemonicParsing="false" onAction="#cancelVote"
           text="Cancel"/>
</AnchorPane>
